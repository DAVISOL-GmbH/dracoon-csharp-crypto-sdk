<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;netstandard2.1;net6.0;net7.0;net8.0;net472;net481</TargetFrameworks>
    <Company>DRACOON GmbH</Company>
    <PackageId>Dracoon.Crypto.Sdk.Net8</PackageId>
    <Title>Dracoon Crypto SDK</Title>
    <Description>A library which implements the client-side encryption of DRACOON. Ported to .NET 8.0 and enhanced by DAVISOL GmbH.</Description>
    <Product>Dracoon Crypto SDK for .NET Standard</Product>
    <Copyright>Copyright Â©2021 Dracoon GmbH</Copyright>
    <AssemblyName>Dracoon.Crypto.Sdk</AssemblyName>
    <RootNamespace>Dracoon.Crypto.Sdk</RootNamespace>
    <PackageTags>dracoon sdk crypto cryptography encryption net48 net6 net8 netstandard2 davisol</PackageTags>
    <Authors>DRACOON Developers, enhancements by DAVISOL GmbH</Authors>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <PackageIcon>DracoonCryptoSdk_NugetImage.png</PackageIcon>
    <PackageProjectUrl>https://github.com/DAVISOL-GmbH/dracoon-csharp-crypto-sdk</PackageProjectUrl>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <PackageIconUrl>https://raw.githubusercontent.com/dracoon/dracoon-csharp-crypto-sdk/master/DracoonCryptoSdk/DracoonCryptoSdk_NugetImage.png</PackageIconUrl>
    <RepositoryUrl>https://github.com/DAVISOL-GmbH/dracoon-csharp-crypto-sdk/tree/v5.1_net8.0</RepositoryUrl>
    <Version>5.1.0-alpha</Version>
    <OutDir></OutDir>
    <GenerateDocumentationFile>True</GenerateDocumentationFile>
    <SignAssembly>False</SignAssembly>
    <PackageReadmeFile>README.md</PackageReadmeFile>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
    <None Include="..\README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
    <None Include="DracoonCryptoSdk_NugetImage.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="BouncyCastle.Cryptography" Version="2.4.0" />
  </ItemGroup>

</Project>